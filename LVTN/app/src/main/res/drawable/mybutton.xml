<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="oval"  >
            <corners android:radius="10dip" />
            <stroke android:width="1dip" android:color="#008080" />
            <gradient android:angle="-90" android:startColor="#008080" android:endColor="#689a92"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="oval"  >
            <corners android:radius="10dip" />
            <stroke android:width="1dip" android:color="#008080" />
            <solid android:color="#008080"/>
        </shape>
    </item>
    <item >
        <shape android:shape="oval"  >
            <corners android:radius="10dip" />
            <stroke android:width="1dip" android:color="#008080" />
            <gradient android:angle="-90" android:startColor="#008080" android:endColor="#58857e" />
        </shape>
    </item>
</selector>
